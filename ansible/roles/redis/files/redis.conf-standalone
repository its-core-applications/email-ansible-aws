tcp-keepalive 60
bind 127.0.0.1
protected-mode yes
syslog-enabled yes
syslog-facility USER
dir /var/lib/redis/

maxmemory-policy volatile-lru
maxmemory-samples 10

save 3600 1000
appendonly yes
auto-aof-rewrite-percentage 100
auto-aof-rewrite-min-size 64mb
aof-load-truncated yes
stop-writes-on-bgsave-error no
