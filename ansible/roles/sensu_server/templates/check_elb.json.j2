{
  "checks": {
    "elb-nodes": {
      "command": "check-elb-nodes.rb -r {{ aws_region }} -n :::elb_name::: -W 66 -C 33",
      "subscribers": [
        "roundrobin:Class_egress"
      ],
      "interval": 120,
      "timeout": 90,
      "occurrences": 2,
      "proxy_requests": {
        "client_attributes": {
          "subscriptions": "eval: value.include?'Class_elb'"
        }
      }
    },
    "elb-metrics": {
      "command": "metrics-elb.rb -r {{ aws_region }} -n :::elb_name:::",
      "subscribers": [
        "roundrobin:Class_egress"
      ],
      "interval": 30,
      "handler": "graphite",
      "proxy_requests": {
        "client_attributes": {
          "subscriptions": "eval: value.include?'Class_elb'"
        }
      }
    }
  }
}
