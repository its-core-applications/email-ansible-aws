{% if aws_region == aws_primary_region %}
{
  "checks": {
    "box-status": {
      "command": "check-box-status",
      "subscribers": [
        "roundrobin:Class_syslog"
      ],
      "interval": 300,
      "source": "box.com"
    },
    "google-status": {
      "command": "check-google-status",
      "subscribers": [
        "roundrobin:Class_syslog"
      ],
      "interval": 300,
      "source": "google.com"
    }
  }
}
{% endif %}
