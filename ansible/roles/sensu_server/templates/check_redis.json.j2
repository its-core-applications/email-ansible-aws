{
  "checks": {
    "redis": {
      "command": "check-redis-ping.rb",
      "subscribers": [
        "redis"
      ],
      "occurrences": 3,
      "interval": 60
    },
    "corvus": {
      "command": "check-redis-ping.rb -p 6380",
      "subscribers": [
        "Class_egress"
      ],
      "occurrences": 3,
      "interval": 60
    },
    "redis-cluster": {
      "command": "check-redis-cluster -c :::name:::",
      "subscribers": [
        "roundrobin:Class_egress"
      ],
      "occurrences": 3,
      "interval": 60,
      "proxy_requests": {
        "client_attributes": {
          "subscriptions": "eval: value.include?'Class_redis_cluster'"
        }
      }
    }
  }
}
